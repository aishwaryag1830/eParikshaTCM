	<queryString language="plsql">
		<![CDATA[SELECT
TO_CHAR(exam_Date, 'DD-MM-YYYY') AS examDate,
(SELECT module_Name
 FROM ePariksha_Modules
 WHERE finalExamScheduleTable.exam_Module_Id = ePariksha_Modules.module_Id
) AS moduleName,
exam_Is_Time_Bound,
exam_Time_Duration,
exam_No_Of_Ques,
(SELECT COUNT(exam_schedule_Id)
 FROM ePariksha_Exam_Schedule tempSchedule
 WHERE
	TO_CHAR(tempSchedule.exam_date, 'DD-MM-YYYY') = TO_CHAR(finalExamScheduleTable.exam_date, 'DD-MM-YYYY')
 AND
	TO_CHAR(tempSchedule.exam_date, 'DD-MM-YYYY') = (SELECT DISTINCT TO_CHAR(ePariksha_Results.result_Exam_Date, 'DD-MM-YYYY')
 																								FROM ePariksha_Results
 																								WHERE
																									TO_CHAR(ePariksha_Results.result_Exam_Date, 'DD-MM-YYYY') = TO_CHAR(tempSchedule.exam_Date, 'DD-MM-YYYY')
																							   )
) AS totalExamsConducted
FROM ePariksha_Exam_Schedule finalExamScheduleTable 
ORDER BY exam_Date]]>
	</queryString>
