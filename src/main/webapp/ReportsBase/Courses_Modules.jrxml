		(SELECT COUNT(exam_ques_Id) FROM ePariksha_Exam_Questions WHERE exam_module_Id = ePariksha_Modules.module_Id) AS totalQuestions,
		(SELECT COUNT(exam_Schedule_Id) FROM ePAriksha_Exam_Schedule WHERE exam_Module_Id = ePariksha_Modules.module_Id) AS totalExamScheduled
