	<queryString language="plsql">
		<![CDATA[SELECT  result_Stud_PRN,
		(SELECT course_Short_Name
		 FROM ePariksha_Courses
		 WHERE ePariksha_Courses.course_Id = finalResultsTable.result_course_Id
		) AS courseShortName,
		(SELECT CONCAT(stud_F_Name, ' ', stud_M_Name, ' ', stud_L_Name) AS candidateFullName
		 FROM ePariksha_Student_Login
		 WHERE ePariksha_Student_Login.stud_PRN = finalResultsTable.result_Stud_PRN
		) AS candidateName,
		result_Marks,
		result_Percentage,
		TO_CHAR(result_Exam_Date, 'DD-MM-YYYY') AS examDate,
		(SELECT module_Id
		 FROM ePariksha_Modules
		 WHERE ePariksha_Modules.module_Id = finalResultsTable.result_module_Id
		) AS moduleId,
		(SELECT module_Name
		 FROM ePariksha_Modules
		 WHERE ePariksha_Modules.module_Id = finalResultsTable.result_module_Id
		) AS moduleName,
		(SELECT COUNT(result_stud_PRN)
		 FROM ePariksha_Results tempResults
		 WHERE result_percentage >= 40.00
		 AND tempResults.result_module_id = finalResultsTable.result_module_id
		 AND TO_CHAR(tempResults.result_Exam_Date, 'DD-MM-YYYY') = TO_CHAR(finalResultsTable.result_Exam_Date, 'DD-MM-YYYY')
		) AS totalPassedCandidates,
		(SELECT COUNT(*)
		 FROM (SELECT DISTINCT result_module_Id,
			   		  TO_CHAR(result_Exam_Date, 'DD-MM-YYYY') AS dateResults
			  		  FROM ePariksha_Results
			  		 ) AS tempTable
		) AS totalExamsConducted
FROM ePariksha_Results finalResultsTable
ORDER BY result_Exam_Date, result_Module_Id]]>
	</queryString>
