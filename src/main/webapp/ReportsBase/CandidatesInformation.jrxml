	<queryString language="plsql">
		<![CDATA[SELECT stud_PRN,
			 CONCAT(stud_F_Name, ' ', stud_M_Name, ' ', stud_L_Name) AS CandidateName,
			 stud_Password,
			 (SELECT DISTINCT(course_Name) AS CourseName FROM ePariksha_Courses WHERE course_id = ePariksha_Student_Login.stud_Course_Id) AS Course,
			 (SELECT DISTINCT(course_Id) AS CourseId FROM ePariksha_Courses WHERE course_id = ePariksha_Student_Login.stud_Course_Id) AS CourseId,
			 (SELECT TO_CHAR(course_Validtill_Date, 'DD-MM-YYYY') AS ValidTillDate FROM ePariksha_Courses WHERE course_id = ePariksha_Student_Login.stud_Course_Id) AS CourseValidTillDate
FROM ePariksha_Student_Login
ORDER BY stud_Course_Id, stud_PRN]]>
