	<queryString language="plsql">
		<![CDATA[SELECT  user_Id,
		CONCAT(user_F_Name, ' ', user_M_Name, ' ', user_L_Name) AS Username,
     	(SELECT course_Name FROM ePariksha_Courses
		 WHERE course_Id = ePariksha_User_Master.user_Course_Id
		) AS courseAssigned,
		user_Email_Id,
		user_M_Number,
		TO_CHAR(user_DOB, 'DD-MM-YYYY') AS DOB,
		user_Gender
FROM ePariksha_User_Master
WHERE user_role_id != '999'
ORDER BY user_Id]]>
	</queryString>
